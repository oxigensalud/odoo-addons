<odoo>
    <record id="connector_product_product_view_form" model="ir.ui.view">
        <field name="name">product.product.connector.form</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view" />
        <field name="arch" type="xml">
            <field name="professional_product" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
            <field name="medical_prescription_required" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
            <field name="website_name" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
            <field name="slug_name" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
            <field name="woocommerce_enabled" position="attributes">
                <attribute name="invisible">1</attribute>
            </field>
        </field>
    </record>

</odoo>
